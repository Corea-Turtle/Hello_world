#include <stdio.h>

main()
{
  unsigned char age = 25;
  double height = 166.7;
  float weight = 58.5;
  
  printf("연령: %d세\n", age);
  printf("신장: %fcm\n", height);
  printf("체중: %fkg\n", weight);
}
/*정수형 
int, unsigned int(-가 없음,시스템에 따라 사이즈가 다름), long, unsighned long(32bit), short, unsigned short(16bit), char, unsigned char(8bit)
  실수형
float (32bit), double(64bit)*/
